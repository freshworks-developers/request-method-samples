{
    "getStarWarsPeople": {
        "schema": {
            "method": "GET",
            "host": "swapi.dev",
            "path": "/api/people"
        }
    },
    "getStarWarsPeopleDetails": {
        "schema": {
            "method": "GET",
            "host": "swapi.dev",
            "path": "/api/people/<%= context.id %>"
        }
    },
    "starwarsStarshipSearch":{
        "schema": {
            "method": "GET",
            "host": "swapi.dev",
            "path": "/api/starships?search=<%= context.starshipname %>"
        }
    },
    "getContacts":{
        "schema":{
            "method":"GET",
            "host":"<%= iparams.subdomain %>.freshdesk.com",
            "path":"/api/v2/contacts",
            "headers": {
                "Authorization": "Basic <%= encode(iparam.api_key) %>",
                "Content-Type": "application/json"
              }
        }
    }
}