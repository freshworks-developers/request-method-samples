{
    "getComicData": {
        "schema": {
            "method": "GET",
            "host": "xkcd.com",
            "path": "/info.0.json"
        }
    },
    "getAssests": {
        "schema": {
            "method": "GET",
            "host": "<%= iparam.subdomain %>.freshservice.com",
            "path": "/api/v2/assets",
            "headers": {
                "Authorization": "Basic <%= encode(iparam.api_key) %>",
                "Content-Type": "application/json"
            }
        },
        "options": {
            "isOAuth": false
        }
    }
}