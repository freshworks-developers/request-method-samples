{
    "getStarWarsPeople": {
        "schema": {
            "method": "GET",
            "host": "swapi.dev",
            "path": "/api/people"
        }
    },
    "getStarWarsPeopleDetails": {
        "schema": {
            "method": "GET",
            "host": "swapi.dev",
            "path": "/api/people/<%= context.id %>"
        }
    },
    "starwarsStarshipSearch":{
        "schema": {
            "method": "GET",
            "host": "swapi.dev",
            "path": "/api/starships?search=<%= context.starshipname %>"
        }
    },
    "getAssests": {
        "schema": {
            "method": "GET",
            "host": "<%= iparam.subdomain %>.freshservice.com",
            "path": "/api/v2/assets",
            "headers": {
                "Authorization": "Basic <%= encode(iparam.api_key) %>",
                "Content-Type": "application/json"
            }
        }
    }
}